<section id="feature">
    <div class="container">
        <div class="section-heading">
            <h1 class="title wow fadeInDown" data-wow-delay=".3s">Offer From Me</h1>
            <p class="wow fadeInDown" data-wow-delay=".5s">
                Lorem ipsum dolor sit amet, consectetur adipisicing elit. Sed,<br> quasi dolores numquam dolor vero ex, tempora commodi repellendus quod laborum.
            </p>
        </div>
        <div class="row">
        <% offers.forEach(function(offer) { %>
            <div class="col-md-4 col-lg-4 col-xs-12">
                <div class="media wow fadeInUp animated" data-wow-duration="500ms" data-wow-delay="<%= offer.delay %>">
                    <div class="media-left">
                        <div class="icon">
                            <i class="<%= offer.iconclass %>"></i>
                        </div>
                    </div>
                    <div class="media-body">
                        <h4 class="media-heading"><%= offer.mediah4 %></h4>
                        <p><%= offer.media %></p>
                    </div>
                </div>
            </div>
         <% }); %>
        </div>
    </div>
</section>